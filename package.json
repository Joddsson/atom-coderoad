{
  "name": "atom-coderoad",
  "main": "./lib/atom/main",
  "version": "0.9.1",
  "description": "Interactive tutorial plugin for Atom",
  "author": "Shawn McKay <shawn.j.mckay@gmail.com>",
  "keywords": [
    "atom",
    "coderoad",
    "tutorial",
    "learn",
    "education"
  ],
  "activationCommands": {
    "atom-workspace": "cr-viewer:toggle"
  },
  "bugs": {
    "url": "https://github.com/coderoad/atom-coderoad"
  },
  "repository": "https://github.com/coderoad/atom-coderoad",
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "files": [
    "package.json",
    "keymaps",
    "lib",
    "menus",
    "styles",
    "*.md"
  ],
  "license": "Apache",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "highlights": "1.3.1",
    "marked": "0.3.5",
    "material-ui": "0.15.0-beta.2",
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "react-redux": "4.4.5",
    "react-tap-event-plugin": "1.0.0",
    "redux": "3.5.2",
    "redux-logger": "2.6.1"
  },
  "scripts": {
    "compile": "tsc"
  }
}
