{
  "name": "atom-coderoad",
  "main": "./lib/atom/main",
  "version": "0.5.4",
  "description": "Interactive tutorial plugin for Atom",
  "author": "Shawn McKay <shawn.j.mckay@gmail.com>",
  "keywords": [
    "atom",
    "coderoad",
    "tutorial",
    "learn",
    "education"
  ],
  "activationCommands": {
    "atom-workspace": "cr-viewer:toggle"
  },
  "bugs": {
    "url": "https://github.com/coderoad/atom-coderoad"
  },
  "repository": "https://github.com/coderoad/atom-coderoad",
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "files": [
    "package.json",
    "keymaps",
    "lib",
    "menus",
    "styles",
    "*.md"
  ],
  "license": "Apache",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "classnames": "2.2.3",
    "highlights": "1.3.1",
    "lodash": "4.6.1",
    "marked": "0.3.5",
    "material-ui": "0.14.4",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-redux": "4.4.0",
    "react-scrollbar": "^0.4.0",
    "react-tap-event-plugin": "0.2.2",
    "redux": "3.3.1"
  },
  "devDependencies": {
    "ava": "^0.12.0",
    "enzyme": "2.1.0",
    "jsdom": "8.1.0",
    "react-addons-test-utils": "0.14.7"
  },
  "scripts": {
    "test": "ava",
    "styles": "lessc src/components/_app.less styles/styles.css",
    "compile": "tsc",
    "build": "npm run styles && npm run compile"
  }
}
